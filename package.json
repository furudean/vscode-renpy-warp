{
	"name": "renpy-warp",
	"displayName": "Ren'Py Warp to Line",
	"description": "warp to current line in renpy",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.87.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "renpy-warp.warp",
				"title": "Warp to current line"
			}
		],
		"configuration": {
			"title": "TypeScript",
			"properties": {
				"renpy-warp.sdkPath": {
					"type": "string",
					"default": "~/renpy-sdk",
					"description": "A path to the Ren'Py SDK directory"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/vscode": "^1.87.0",
		"@types/mocha": "^10.0.6",
		"@types/node": "18.x",
		"eslint": "^8.56.0",
		"typescript": "^5.3.3",
		"@vscode/test-cli": "^0.0.6",
		"@vscode/test-electron": "^2.3.9"
	}
}
